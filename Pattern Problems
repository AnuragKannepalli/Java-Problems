        System.out.print("Enter no of columns: ");
        int n= scanner.nextInt();
        for(int i=0;i<=n;i++){
            for(int j=0;j<i;j++){
                System.out.print("*");
            }
            
            System.out.println(" ");
        }
        
    }
}

