import java.io.*;
import java.util.*;
class Pattern1 {
    public static void main(String[] args) {
        Scanner scanner= new Scanner(System.in);
        System.out.print("Enter no of columns: ");
        int n= scanner.nextInt();
        for(int i=1;i<=n;i++){
            for(int j=1;j<=i;j++){
                System.out.print(j);
            }
            
            System.out.println(" ");
        }
        
    }
}

