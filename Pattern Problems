import java.io.*;
import java.util.*;
class Pattern1 {
    public static void main(String[] args) {
        Scanner scanner= new Scanner(System.in);
        System.out.print("Enter no of columns: ");
        int n= scanner.nextInt();
        int a=65;
        char ch1='A';
        for(int i=0;i<n;i++){
            for(int j=0;j<=i;j++){
                a=65+i;
                System.out.print((char)a);
                
            }
            
            System.out.println(" ");
            
            
        }
    }
}

